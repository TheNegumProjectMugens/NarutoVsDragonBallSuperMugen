tension 	= 110;h_dist
tensionhigh = 350;top_dist
tensionlow = 60 ;bottom_dist
overdrawhigh =100
overdrawlow = 100
cuthigh = 0
cutlow = 0
zoomin =.9
zoomout =.46


;--------------------------------------------------------
[PlayerInfo]
;--- Player 1 ---
 ;Player 1 starting coordinates.
 ;p1startx is typically -70 and p2startx is 70.
 ;p1starty and p1startz should be 0.
p1startx = -140          ;Starting x coordinates
p1starty = 0            ;Starting y coordinates
p1startz = 0            ;Starting z coordinates
p1facing = 1            ;Direction player faces: 1=right, -1=left

;--- Player 2 ---
p2startx = 140
p2starty = 0
p2startz = 0
p2facing = -1

;--- Player 3 ---
p3startx = -240
p3starty = 0
p3facing = 1

;--- Player 4 ---
p4startx = 240
p4starty = 0
p4facing = -1


;--- Common ---
 ;Don't change these values.
leftbound  = -4000;Left bound (x-movement)
rightbound =  4000 ;Right bound
topbound  =  300     ;Top bound (z-movement)
botbound  =  0     ;Bottom bound

;--------------------------------------------------------
[Scaling]
 ;No need to change these values
topz     = 0       ;Top z-coordinate for scaling
botz     = 50      ;Bottom z-coordinate for scaling
topscale = 1       ;Scale to use at top
botscale = 1.2     ;Scale to use at bottom

;--------------------------------------------------------
[Bound]
 ;Distance from left/right edge of screen that player can move to
 ;Typically 15
screenleft = 45  ;Dist from left of screen that player can move to
screenright = 45  ;Right edge

;--------------------------------------------------------
[StageInfo]
zoffset = 310
autoturn = 1
resetBG = 1
localcoord=640,360
xscale = 1
yscale = 1

;--------------------------------------------------------
[Shadow]
intensity = 0
color = 0,0,0
yscale = -.1
fade.range = -400,-100
[Reflection]
intensity = 130

;--------------------------------------------------------
[Music]

bgmusic = sound/Ascension To Glory.mp3
bgmvolume = 255

[BGdef]
